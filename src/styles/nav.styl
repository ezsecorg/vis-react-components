// Navigation bar

nav
  width 100%
  margin 0 0 15px 0
  padding 15px 10px
  background-color #496873

nav ul
  overflow hidden
  position relative
  margin 0
  padding 0

nav ul li
  list-style-type none
  display inline
  padding 0 20px

nav ul li a
  color #eee
  text-align center
  text-decoration none

nav ul li .active
  color white
  font-weight 800
